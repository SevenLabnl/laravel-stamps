{
    "name": "7lab/laravel-stamps",
    "description": "Adds userstamps to models and migrations and handles it automatically",
    "license": "MIT",
    "authors": [
        {
            "name": "7Lab",
            "email": "info@7lab.nl",
            "homepage": "https://7lab.nl"
        }
    ],
    "homepage": "https://github.com/7Lab/laravel-stamps",
    "keywords": [
        "Laravel",
        "Stamps",
        "Userstamps"
    ],
    "require": {
        "illuminate/support": "~7|~8|~9|~10|~11"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "SevenLab\\Stamps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SevenLab\\Stamps\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SevenLab\\Stamps\\StampsServiceProvider"
            ],
            "aliases": {
                "Stamps": "SevenLab\\Stamps\\Facades\\Stamps"
            }
        }
    }
}
